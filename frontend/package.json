{
  "name": "spa_frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "check": "pnpm lint && pnpm prettier:check && tsc --noEmit --skipLibCheck",
    "check:ts": "tsc --noEmit --skipLibCheck",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "openapi": "curl http://localhost:8080/v3/api-docs>api-doc.json",
    "openapi:generate": "typed-openapi -r zod ../../0_backend/generated-api-doc.yaml -o src/components/api-types.ts && prettier --write src/components/api-types.ts",
    "openapi:watch": "onchange -i ../../0_backend/generated-api-doc.yaml -- pnpm openapi:generate"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.59.19",
    "@tanstack/react-query-devtools": "^5.59.19",
    "@tanstack/react-router": "^1.78.3",
    "bootstrap-icons": "^1.11.3",
    "date-fns": "^3.6.0",
    "ky": "^1.7.2",
    "react": "19.0.0-rc-4d577fd2-20241104",
    "react-dom": "19.0.0-rc-4d577fd2-20241104",
    "react-error-boundary": "^4.1.2",
    "tailwind-merge": "^2.5.4",
    "zod": "^3.23.8",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.59.7",
    "@tanstack/router-devtools": "^1.78.3",
    "@tanstack/router-plugin": "^1.78.3",
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "babel-plugin-react-compiler": "19.0.0-beta-63b359f-20241101",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-compiler": "19.0.0-beta-63b359f-20241101",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.14",
    "google-fonts-helper": "^3.6.0",
    "google-fonts-offline": "^0.1.5",
    "onchange": "^7.1.0",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.14",
    "typed-openapi": "^0.4.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.10"
  },
  "overrides": {
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc"
  },
  "packageManager": "pnpm@9.9.0+sha512.60c18acd138bff695d339be6ad13f7e936eea6745660d4cc4a776d5247c540d0edee1a563695c183a66eb917ef88f2b4feb1fc25f32a7adcadc7aaf3438e99c1"
}
